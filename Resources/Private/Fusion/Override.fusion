prototype(Neos.Neos:Page).outdatedBrowser = Jonnitto.OutdatedBrowser:Component {
    @position = 'before closingBodyTag'
    @if {
        isNotCrawler = ${Configuration.setting('Jonnitto.OutdatedBrowser.disableForCrawler') ? !Jonnitto.OutdatedBrowser.isCrawler(request.httpRequest) : true}
        isLive = ${node.context.live}
    }
}
