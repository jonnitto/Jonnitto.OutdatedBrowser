{
    "name": "jonnitto/outdatedbrowser",
    "type": "neos-plugin",
    "license": "GPL-3.0-or-later",
    "description": "Outdated Browser for Neos CMS",
    "keywords": [
        "flow",
        "neos",
        "Outdated Browser"
    ],
    "authors": [
        {
            "name": "Jon Uhlmann",
            "email": "me@jonnitto.ch",
            "homepage": "https://twitter.com/jonnitto",
            "role": "Developer"
        }
    ],
    "require": {
        "neos/neos": "^4.3 || ^5.0 || ^7.0",
        "neos/fusion-afx": "^1.1 || ^7.0",
        "jaybizzle/crawler-detect": "^1.2"
    },
    "conflict": {
        "dotpulse/outdatedbrowser": "*"
    },
    "autoload": {
        "psr-4": {
            "Jonnitto\\OutdatedBrowser\\": "Classes"
        }
    },
    "extra": {
        "installer-name": "Jonnitto.OutdatedBrowser",
        "neos": {
            "package-key": "Jonnitto.OutdatedBrowser"
        }
    }
}
